{
  "name": "projectile-data-lab",
  "version": "1.1.0-dev.6",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/projectile-data-lab.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "PROJECTILE_DATA_LAB",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet",
      "phet-io"
    ],
    "phetLibs": [
      "bamboo"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsInteractiveDescription": true,
      "supportsSound": true
    },
    "screenNameKeys": [
      "PROJECTILE_DATA_LAB/screen.variability",
      "PROJECTILE_DATA_LAB/screen.sources",
      "PROJECTILE_DATA_LAB/screen.measures",
      "PROJECTILE_DATA_LAB/screen.sampling"
    ],
    "phet-io": {
      "wrappers": [
        "phet-io-sim-specific/repos/projectile-data-lab/wrappers/codap"
      ],
      "publicWrappers": [
        "codap"
      ],
      "compareDesignedAPIChanges": true
    },
    "published": true
  }
}